{
  "name": "@activitree/meteor-mongoosed",
  "version": "0.0.1",
  "description": "Mongoose adapted for use with MeteorJS",
  "directories": {
    "lib": "./lib/mongoose"
  },
  "main": "./index.js",
  "types": "./types/index.d.ts",
  "engines": {
    "node": ">=14.20.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activitree/meteor-mongoosed.git"
  },
  "keywords": [
    "meteor",
    "meteorjs",
    "mongoose"
  ],
  "author": "Paul Ailincai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/activitree/meteor-mongoosed/issues"
  },
  "config": {
    "mongodbMemoryServer": {
      "disablePostinstall": true
    }
  },
  "tsd": {
    "directory": "test/types",
    "compilerOptions": {
      "esModuleInterop": false,
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "strictPropertyInitialization": false,
      "noImplicitAny": false,
      "strictNullChecks": true,
      "module": "commonjs",
      "target": "ES2017"
    }
  },
  "homepage": "https://github.com/activitree/meteor-mongoosed#readme"
}
